<% if @restaurant.errors.any? %>
    <section id="'errors">
      <h2>
        <% pluralize(@restaurant.errors.count, "error") %></h2>
      <ul>
        <% @restaurant.errors.full_messages.each do |message| %>
            <li><%= message %></li>
        <% end %>
      </ul>
    </section>
<% end %>


<%= render "form" %>

